{
  "name": "public-website",
  "description": "DoneOps Public Website",
  "version": "0.1.0",
  "author": "DoneOps",
  "dependencies": {
    "bulma": "^1.0.3",
    "decap-cms-app": "^3.4.0",
    "gatsby": "5.14.1",
    "gatsby-image": "^3.11.0",
    "gatsby-plugin-decap-cms": "^4.0.4",
    "gatsby-plugin-google-gtag": "^5.14.0",
    "gatsby-plugin-netlify": "^5.1.1",
    "gatsby-plugin-purgecss": "^6.2.1",
    "gatsby-plugin-react-helmet": "^6.14.0",
    "gatsby-plugin-remove-fingerprints": "^0.0.2",
    "gatsby-plugin-sass": "^6.14.0",
    "gatsby-plugin-sharp": "^5.14.0",
    "gatsby-plugin-sitemap": "^6.14.0",
    "gatsby-plugin-sri": "^1.1.0",
    "gatsby-remark-copy-linked-files": "^6.14.0",
    "gatsby-remark-images": "^7.14.0",
    "gatsby-remark-relative-images": "^2.0.5",
    "gatsby-source-filesystem": "^5.14.0",
    "gatsby-theme-codebushi": "1.0.0",
    "gatsby-transformer-remark": "^6.14.0",
    "gatsby-transformer-sharp": "^5.14.0",
    "lodash": "^4.17.21",
    "lodash-webpack-plugin": "^0.11.6",
    "postcss-sass": "^0.5.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-anchor-link-smooth-scroll": "^1.0.12",
    "react-dom": "^18.2.0",
    "react-google-recaptcha": "^3.1.0",
    "react-helmet": "^6.1.0",
    "sass": "^1.83.1",
    "stylelint": "^16.12.0",
    "stylelint-config-standard": "^36.0.1",
    "uuid": "^11.0.4"
  },
  "keywords": [
    "gatsby",
    "react",
    "javescript"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "clean": "gatsby clean",
    "start": "npm run develop",
    "build": "GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages",
    "develop": "npm run clean && gatsby develop",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "lint-staged": "lint-staged",
    "format": "prettier \"{gatsby-*.js,src/**/*.js}\"",
    "format:fix": "prettier --write \"{gatsby-*.js,src/**/*.js}\"",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DoneOps/public-website"
  },
  "bugs": {
    "url": "https://github.com/DoneOps/public-website/issues"
  },
  "engines": {
    "node": "^22.0.0",
    "yarn": "^4.0.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.25.9",
    "eslint": "^9.17.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.3",
    "eslint-plugin-react-hooks": "^5.1.0",
    "gatsby-cli": "5.14.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.3.0",
    "prettier": "^3.4.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.+(js|jsx)": [
      "eslint --fix"
    ],
    "*.+(json|css|md)": [
      "prettier --write"
    ]
  },
  "packageManager": "yarn@4.6.0"
}
