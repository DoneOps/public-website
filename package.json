{
  "name": "public-website",
  "description": "DoneOps Public Website",
  "version": "0.1.0",
  "author": "DoneOps",
  "dependencies": {
    "bulma": "^0.9.4",
    "gatsby": "4.18.2",
    "gatsby-image": "^3.11.0",
    "gatsby-plugin-google-gtag": "^4.18.0",
    "gatsby-plugin-hubspot": "^2.0.0",
    "gatsby-plugin-netlify": "^5.0.0",
    "gatsby-plugin-netlify-cms": "^6.18.0",
    "gatsby-plugin-purgecss": "^6.1.2",
    "gatsby-plugin-react-helmet": "^5.18.0",
    "gatsby-plugin-remove-fingerprints": "^0.0.2",
    "gatsby-plugin-sass": "^5.18.1",
    "gatsby-plugin-sharp": "^4.18.1",
    "gatsby-plugin-sitemap": "^5.18.1",
    "gatsby-plugin-sri": "^1.1.0",
    "gatsby-remark-copy-linked-files": "^5.18.0",
    "gatsby-remark-images": "^6.18.1",
    "gatsby-remark-relative-images": "^2.0.2",
    "gatsby-source-filesystem": "^4.18.1",
    "gatsby-theme-codebushi": "1.0.0",
    "gatsby-transformer-remark": "^5.18.1",
    "gatsby-transformer-sharp": "^4.20.0",
    "lodash": "^4.17.21",
    "lodash-webpack-plugin": "^0.11.6",
    "netlify-cms-app": "^2.15.72",
    "postcss-sass": "^0.5.0",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-anchor-link-smooth-scroll": "^1.0.12",
    "react-dom": "^17.0.2",
    "react-google-recaptcha": "^2.1.0",
    "react-helmet": "^6.1.0",
    "sass": "^1.53.0",
    "stylelint": "^14.9.1",
    "stylelint-config-standard": "^26.0.0",
    "uuid": "^8.3.2"
  },
  "keywords": [
    "gatsby",
    "react",
    "javescript"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "clean": "gatsby clean",
    "start": "npm run develop",
    "build": "GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages",
    "develop": "npm run clean && gatsby develop",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "lint-staged": "lint-staged",
    "format": "prettier \"{gatsby-*.js,src/**/*.js}\"",
    "format:fix": "prettier --write \"{gatsby-*.js,src/**/*.js}\"",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DoneOps/public-website"
  },
  "bugs": {
    "url": "https://github.com/DoneOps/public-website/issues"
  },
  "engines": {
    "node": "^17.2.0",
    "yarn": "^3.0.0"
  },
  "devDependencies": {
    "@netlify/plugin-gatsby": "^3.3.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.19.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "gatsby-cli": "4.18.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.+(js|jsx)": [
      "eslint --fix"
    ],
    "*.+(json|css|md)": [
      "prettier --write"
    ]
  }
}
