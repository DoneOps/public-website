{
  "name": "public-website",
  "description": "DoneOps Public Website",
  "version": "0.1.0",
  "author": "DoneOps",
  "dependencies": {
    "bulma": "^0.9.0",
    "gatsby": "^2.32.8",
    "gatsby-image": "^2.2.43",
    "gatsby-plugin-netlify": "^2.1.34",
    "gatsby-plugin-netlify-cache": "^1.2.0",
    "gatsby-plugin-netlify-cms": "^4.1.41",
    "gatsby-plugin-purgecss": "^5.0.0",
    "gatsby-plugin-react-helmet": "^3.1.23",
    "gatsby-plugin-sass": "^3.0.0",
    "gatsby-plugin-sharp": "^2.14.3",
    "gatsby-plugin-sri": "^1.1.0",
    "gatsby-remark-copy-linked-files": "^2.1.39",
    "gatsby-remark-images": "^3.1.49",
    "gatsby-remark-relative-images": "^2.0.2",
    "gatsby-source-filesystem": "^2.1.54",
    "gatsby-theme-codebushi": "1.0.0",
    "gatsby-transformer-remark": "^2.6.58",
    "gatsby-transformer-sharp": "^2.3.18",
    "lodash": "^4.17.15",
    "lodash-webpack-plugin": "^0.11.5",
    "netlify-cms-app": "^2.15.17",
    "node-sass": "^5.0.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-anchor-link-smooth-scroll": "^1.0.12",
    "react-dom": "^17.0.1",
    "react-google-recaptcha": "^2.0.1",
    "react-helmet": "^6.0.0",
    "uuid": "^8.0.0"
  },
  "keywords": [
    "gatsby",
    "react",
    "javescript"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "clean": "gatsby clean",
    "start": "npm run develop",
    "build": "GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages",
    "develop": "npm run clean && gatsby develop",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier \"{gatsby-*.js,src/**/*.js}\"",
    "format:fix": "prettier --write \"{gatsby-*.js,src/**/*.js}\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DoneOps/public-website"
  },
  "bugs": {
    "url": "https://github.com/DoneOps/public-website/issues"
  },
  "engines": {
    "node": "^14.15.0",
    "yarn": "^1.22.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.22.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "gatsby-cli": "^2.11.1",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.8",
    "prettier": "^2.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.+(js|jsx)": [
      "eslint --fix"
    ],
    "*.+(json|css|md)": [
      "prettier --write"
    ]
  }
}
