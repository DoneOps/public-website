{
  "name": "public-website",
  "description": "DoneOps Public Website",
  "version": "0.1.0",
  "author": "DoneOps",
  "dependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/typography": "^0.5.15",
    "autoprefixer": "^10.4.20",
    "change-case": "^5.4.4",
    "cssnano": "^7.0.6",
    "gatsby": "^5.15.0",
    "gatsby-plugin-google-gtag": "^5.15.0",
    "gatsby-plugin-image": "^3.15.0",
    "gatsby-plugin-postcss": "^6.15.0",
    "gatsby-plugin-sharp": "^5.15.0",
    "gatsby-plugin-sitemap": "^6.15.0",
    "gatsby-plugin-typescript": "^5.15.0",
    "gatsby-remark-copy-linked-files": "^6.15.0",
    "gatsby-remark-images": "^7.15.0",
    "gatsby-source-filesystem": "^5.15.0",
    "gatsby-transformer-remark": "^6.15.0",
    "gatsby-transformer-sharp": "^5.15.0",
    "postcss": "^8.4.47",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-anchor-link-smooth-scroll": "^1.0.12",
    "react-dom": "^18.3.1",
    "react-google-recaptcha": "^3.1.0",
    "tailwindcss": "^3.4.0"
  },
  "keywords": [
    "gatsby",
    "react",
    "javescript"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "clean": "gatsby clean",
    "start": "npm run develop",
    "build": "GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages",
    "develop": "npm run clean && gatsby develop",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "lint-staged": "lint-staged",
    "format": "prettier \"{gatsby-*.js,src/**/*.js}\"",
    "format:fix": "prettier --write \"{gatsby-*.js,src/**/*.js}\"",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DoneOps/public-website"
  },
  "bugs": {
    "url": "https://github.com/DoneOps/public-website/issues"
  },
  "engines": {
    "node": "^24.0.0",
    "yarn": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/eslint-parser": "^7.28.5",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "gatsby-cli": "^5.15.0",
    "husky": "^9.1.1",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "typescript": "^5.7.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.+(js|jsx)": [
      "eslint --fix"
    ],
    "*.+(json|css|md)": [
      "prettier --write"
    ]
  },
  "packageManager": "yarn@4.7.0"
}
